<?xml version='1.0' encoding='utf-8'?>
<widget id="de.corrently.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Corrently</name>
    <description>
    </description>
    <author email="info@helmundwalter.de" href="http://helmundwalter.de">
        Helm &amp; Walter IT-Solutions GbR
    </author>
    <content src="splashscreen.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="mdpi" height="57" platform="android" src="www/imgs/logo.png" width="57" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
